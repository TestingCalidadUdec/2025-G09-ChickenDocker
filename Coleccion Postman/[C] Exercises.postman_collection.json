							"// Respuesta en formato JSON",
							"pm.test(\"La respuesta tiene formato JSON\", function () {",
							"    pm.response.to.be.json;",
							"});",
							"",
							"pm.test(\"Response has expected validation error structure\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.have.property(\"detail\");",
							"    pm.expect(json.detail).to.be.an(\"array\").that.is.not.empty;",
							"",
							"    const error = json.detail[0];",
							"    pm.expect(error).to.have.property(\"type\");",
							"    pm.expect(error).to.have.property(\"loc\");",
							"    pm.expect(error).to.have.property(\"msg\");",
							"    pm.expect(error).to.have.property(\"input\");",
							"    pm.expect(error).to.have.property(\"ctx\");",
							"    pm.expect(error.ctx).to.have.property(\"expected\");",
							"});",
							"",
							"pm.test(\"Validation error is for exercise_type\", function () {",
							"    const json = pm.response.json();",
							"    const error = json.detail[0];",
							"    pm.expect(error.loc).to.include.members([\"body\", \"exercise_type\"]);",
							"    pm.expect(error.msg).to.include(\"WEIGHT_BASED\", \"TIME_BASED\");",
